CREATE TABLE `t_product` (
  `id` bigint(20) unsigned NOT NULL,
  `name` varchar(50) NOT NULL COMMENT '商品名称',
  `price` int(11) NOT NULL COMMENT '价格 分',
  `stock` int(11) NOT NULL COMMENT '库存',
  `state` tinyint(4) NOT NULL COMMENT '状态 0 下架 1 上架',
  `create_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '加入时间',
  `update_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '修改时间',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=6 DEFAULT CHARSET=utf8mb4 COMMENT='商品表'


CREATE TABLE `t_user` (
  `id` bigint(20) unsigned NOT NULL,
  `nick_name` varchar(50) NOT NULL COMMENT '用户昵称',
  `mobile` char(11) NOT NULL COMMENT '手机',
  `create_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '加入时间',
  `update_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '修改时间',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=8 DEFAULT CHARSET=utf8mb4 COMMENT='用户表'