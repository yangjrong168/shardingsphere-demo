CREATE TABLE `t_order_0` (
  `id` bigint NOT NULL,
  `order_sn` varchar(64) NOT NULL,
  `member_id` bigint NOT NULL,
  `create_time` timestamp	NOT NULL,
  `status` int DEFAULT 0 ,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB  DEFAULT CHARSET=utf8;

